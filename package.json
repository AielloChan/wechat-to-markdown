{
  "name": "@aiello/wechat-to-markdown",
  "version": "1.2.3",
  "description": "解析微信文章 URL 为 markdown",
  "author": "Aiello Chan<aiello.chan@gmail.com>",
  "keywords": [
    "wechat to markdown",
    "markdown"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@ryan-liu/eslint-config-jsx-config": "^1.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/turndown": "^5.0.1",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "inquirer": "^8.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "tsup": "^5.4.0",
    "typescript": "^4.4.3",
    "vitest": "^1.3.1"
  },
  "scripts": {
    "dev": "yarn build --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "lint": "lint-staged",
    "pub": "node tools/publish.js",
    "test": "vitest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --quiet"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "tsup": {
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "external": [
      "html2markdown"
    ]
  },
  "dependencies": {
    "axios": "^0.22.0",
    "cheerio": "^1.0.0-rc.10",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2"
  }
}
